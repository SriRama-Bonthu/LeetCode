class Solution {
    public long[] findPrefixScore(int[] nums) {
        long[] res=new long[nums.length];
        res[0]=nums[0]+nums[0];
        int max=nums[0];
        for(int i=1;i<nums.length;i++){
            if(nums[i]>max){
                max=nums[i];
            }
            res[i]=res[i-1]+max+nums[i];
        }
        return res;
    }
}
