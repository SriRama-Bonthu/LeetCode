class Solution {
    public int minTimeToVisitAllPoints(int[][] points) {
        int s=0;
        for(int i=1;i<points.length;i++){
            int x=points[i][0];
            int y=points[i][1];
            int befx=points[i-1][0];
            int befy=points[i-1][1];
            s+=Math.max(Math.abs(x-befx),Math.abs(y-befy));
        }
        return s;
    }
}
