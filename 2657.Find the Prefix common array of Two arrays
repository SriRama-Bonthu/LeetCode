class Solution {
    public int[] findThePrefixCommonArray(int[] A, int[] B) {
        int n=A.length;
        int[] freq=new int[51];
        int[] l=new int[n];
        int c=0;

        for (int i=0;i<n;i++) {
            freq[A[i]]++;
            freq[B[i]]++;
            if(A[i]==B[i]){
                c++;
                l[i]=c;
                continue;
            }
            if(freq[A[i]]>=2){
                c++;
            }
            if(freq[B[i]]>=2){
                c++;
            }
            l[i]=c;
        }
        return l;
    }
}
