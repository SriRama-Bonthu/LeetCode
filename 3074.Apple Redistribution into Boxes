class Solution {
    public int minimumBoxes(int[] apple, int[] capacity) {
        int s=0;
        for(int i=0;i<apple.length;i++){
            s+=apple[i];
        }
        Arrays.sort(capacity);
        int c=0;
        int s2=0;
        int j=capacity.length-1;
        while(j>=0 && s2<s){
            s2+=capacity[j];
            c++;
            j--;
        }
        return c;
    }
}
