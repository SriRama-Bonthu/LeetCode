class Solution {
    public int minOperations(int[] nums) {
        int i=0;
        int j=0;
        int c=0;
        while(i<nums.length-2){
            if(nums[i]==0){
                nums[i]=1;
                nums[i+1]=nums[i+1]^1;
                nums[i+2]=nums[i+2]^1;
                c++;
            }
            i++;
        }
        for(int l=0;l<nums.length;l++){
            if(nums[l]==0){
                return -1;
            }
        }
        return c;
    }
}
