class Solution {
    public int kthFactor(int n, int k) {
        int c=0;
        int i=1;
        int res=1;
        while(c!=k && i<=n){
            if(n%i==0){
                res=i;
                c++;
            }
            i++;
        }
        if(c!=k){
            return -1;
        }
        return res;
    }
}
