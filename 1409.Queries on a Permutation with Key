class Solution {
    public int[] processQueries(int[] queries, int m) {
        int[] p=new int[m];
        for(int i=0;i<m;i++){
            p[i]=i+1;
        }
        int[] res=new int[queries.length];
        for(int i=0;i<queries.length;i++){
            int l=queries[i];
            int j=0;
            while(p[j]!=l){
                j++;
            }
            int temp=p[0];
            p[0]=p[j];
            int c=1;
            while(c<=j){
                int t=p[c];
                p[c]=temp;
                temp=t;
                c++;
            }
            res[i]=j;
        }
        return res;
    }
}
