class Solution {
    public int divisorSum(int n){
        int c=0;
        int s=0;
        for(int i=1;i<=n;i++){
            if(n%i==0){
                s+=i;
                c++;
            }
            if(c>4){
                break;
            }
        }
        if(c==4){
            return s;
        }
        return 0;
    }
    public int sumFourDivisors(int[] nums) {
        int s=0;
        for(int i=0;i<nums.length;i++){
            s+=divisorSum(nums[i]);
        }
        return s;
    }
}
