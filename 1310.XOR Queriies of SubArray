class Solution {
    public int[] xorQueries(int[] arr, int[][] queries) {
        int[] ans=new int[queries.length];
        int[] pre=new int[arr.length];
        pre[0]=arr[0];
        for(int i=1;i<arr.length;i++){
            pre[i]=pre[i-1]^arr[i];
        }
        for(int i=0;i<queries.length;i++){
            int L=queries[i][0];
            int R=queries[i][1];
            if(L==0){
                ans[i]=pre[R];
            }
            else{
                ans[i]=pre[R]^pre[L-1];
            }
        }
        return ans;
    }
}

